<h1>Listing applications</h1>

<table>
  <thead>
    <tr>
      <th>Immo</th>
      <th>User</th>
      <th>Title</th>
      <th>Picture url</th>
      <th>Price</th>
      <th>Living space</th>
      <th>Rooms</th>
      <th>Address</th>
      <th>Email</th>
      <th>Mobile</th>
      <th>Landline</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @applications.each do |application| %>
      <tr>
        <td><%= application.immo_id %></td>
        <td><%= application.user_id %></td>
        <td><%= application.title %></td>
        <td><%= application.picture_url %></td>
        <td><%= application.price %></td>
        <td><%= application.living_space %></td>
        <td><%= application.rooms %></td>
        <td><%= application.address %></td>
        <td><%= application.email %></td>
        <td><%= application.mobile %></td>
        <td><%= application.landline %></td>
        <td><%= link_to 'Show', application %></td>
        <td><%= link_to 'Edit', edit_application_path(application) %></td>
        <td><%= link_to 'Destroy', application, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Application', new_application_path %>
